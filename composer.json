{
    "name": "jkujawski/laravel-taxjar",
    "description": "Laravel TaxJar Wrapper",
    "keywords": [
        "laravel",
        "taxjar",
        "tax",
        "jar"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jakub Kujawski",
            "email": "me@jakub.pro"
        }
    ],
    "autoload": {
        "psr-4": {
            "Jkujawski\\TaxJar\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jkujawski\\TaxJar\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "taxjar/taxjar-php": "^1.10",
        "illuminate/support": "^5.2"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "^8.5",
        "graham-campbell/testbench": "^5.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jkujawski\\TaxJar\\TaxJarServiceProvider"
            ]
        }
    }
}
